{
	"name": "i18n-sample",
	"private": true,
	"displayName": "i18n-sample",
	"description": "Sample that shows how to localize an extension",
	"repository": "https://github.com/Microsoft/vscode-extension-samples/i18n-sample",
	"version": "0.1.0",
	"publisher": "vscode-i18n-sample",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.sayHello",
		"onCommand:extension.sayBye"
	],
	"main": "./dist/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.sayHello",
				"title": "%extension.sayHello.title%"
			},
			{
				"command": "extension.sayBye",
				"title": "%extension.sayBye.title%"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compiledev",
		"watch": "webpack --mode production --vscode-nls --watch --info-verbosity verbose",
		"compile": "gulp build",
		"compiledev": "gulp build && webpack --mode production --vscode-nls",
		"clean": "gulp clean",
		"package": "vsce package"
	},
	"devDependencies": {
		"@types/node": "^12.0.1",
		"@types/vscode": "^1.34.0",
		"copy-webpack-plugin": "^5.0.5",
		"del": "^4.1.1",
		"event-stream": "^4.0.1",
		"gulp": "^4.0.2",
		"gulp-filter": "^5.1.0",
		"gulp-sourcemaps": "^2.6.5",
		"gulp-typescript": "^5.0.1",
		"merge-options": "^2.0.0",
		"run-sequence": "^2.2.1",
		"ts-loader": "^6.0.4",
		"tslint": "^5.16.0",
		"typescript": "^3.5.1",
		"vsce": "^1.61.0",
		"vscode-nls-dev": "^3.2.6",
		"webpack": "^4.39.2",
		"webpack-cli": "^3.3.7",
		"webpack-stream": "^5.2.1"
	},
	"dependencies": {
		"vscode-nls": "^4.1.1"
	}
}
